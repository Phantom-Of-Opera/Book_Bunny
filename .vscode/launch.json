{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Capstone Web App",
			"program": "${workspaceFolder}/index.js",
			"env": {
				"NODE_ENV": "development"
			},
			"cwd": "${workspaceFolder}",
			"skipFiles": ["<node_internals>/**"],
			"console": "integratedTerminal"
		}
	]
}
